{
	"compilerOptions": {
		"strict": true,
		"alwaysStrict": true,
		"exactOptionalPropertyTypes": true,
		"noUncheckedIndexedAccess": true,
		"target": "esnext",
		"module": "esnext",
		"sourceMap": true,
		"removeComments": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"declaration": true,
		"baseUrl": "src"
	}
}